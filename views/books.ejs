<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>

        <link rel="stylesheet" href="css/style.css"/>
        <link rel="stylesheet" href="css/media_queries.css"/>
        <link rel="icon" href="" type="image/icon type">

        <title>Bookify</title>
    </head>
    <body>
        <section>
            <div class="container">
                 <div class="header">
                    <a href="#"><img class="logo" src="logo.svg"></a>
                    <a href="/"><button class="search-btn">Search again</button></a>
                 </div>
                 <div class="">
                 <div class="card">
                     <ul class="list-container">
                        <%  data["items"].forEach(function(book){ %>
                         <li><a href="<%= book["volumeInfo"].previewLink %>">
                             <div class="book-info">
                                <div>Title: <%= book["volumeInfo"].title %></div>
                                <div>Authors: <%= book["volumeInfo"].authors %></div>
                                <div>Published Date: <%= book["volumeInfo"].publishedDate %></div>
                             
                        <%  })  %>
                             </div>
                            </a>
                        </li>
                       
                     </ul>
                 </div>
                 </div>
            </div>
        </section>
    </body>
</html>
